{
  "expo": {
    "name": "Baby Doodle Pad",
    "slug": "Baby Doodle Pad",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F8FF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.babyplaypad",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMotionUsageDescription": "This app uses motion sensors to detect shake gestures for clearing the canvas.",
        "UIRequiresFullScreen": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#F8F8FF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.babyplaypad",
      "permissions": [
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access Face ID for secure PIN verification."
        }
      ]
    ],
    "scheme": "babyplaypad",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "Baby Doodle Pad"
}